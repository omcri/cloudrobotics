<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated omcri -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="omcri.presentation.OmcriModelWizardID"
            name="%_UI_OmcriModelWizard_label"
            class="omcri.presentation.OmcriModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/OmcriModelFile.gif">
         <description>%_UI_OmcriModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated omcri -->
      <editor
            id="omcri.presentation.OmcriEditorID"
            name="%_UI_OmcriEditor_label"
            icon="icons/full/obj16/OmcriModelFile.gif"
            extensions="omcri"
            class="omcri.presentation.OmcriEditor"
            contributorClass="omcri.presentation.OmcriActionBarContributor">
      </editor>
   </extension>

</plugin>
